#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	setlocale(0, "");
	int N;
	cout << "Дана строка с символами:" << endl;
	char line[]="wwwwwwwwww";
	cout << line << endl;
	cout << "Строка после изменений: " << endl;
	for (int i = 0; i < strlen(line); i++)
	{
		cout << line[i] << " ";
	}
}
