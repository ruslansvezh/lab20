#include <iostream>
#include <string>
#include <cmath>
using namespace std;
int main()
{
	setlocale(0, "");
	int num = 0;
	string line;
	char simbol = 'c';

	cout << "Введите строку: ";
	getline(cin, line);


	for (int i = 0; i < line.size(); i++)
	{
		if (line[i] == simbol)
		{
			cout << line[i] << simbol;
		}
		else
		{
			cout << line[i];
		}
	}
}
